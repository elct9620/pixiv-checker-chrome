{
  "name": "Pixiv Checker",
  "description": "A Pixiv bookmark illust new illustration checker.",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "*://*.pixiv.net/",
    "notifications",
    "storage",
    "tabs",
    "alarms"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png"
    },
    "default_title": "Pixiv Checker"
  }
}
